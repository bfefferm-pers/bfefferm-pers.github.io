@article{pahng2025diffeomorph,
  title={DiffeoMorph: Learning to Morph 3D Shapes Using Differentiable Agent-Based Simulations},
  author={Pahng, Seong Ho and Guan, Guoye and Fefferman, Benjamin and Hormoz, Sahand},
  journal={arXiv preprint arXiv:2512.17129},
  year={2025},
  url={https://arxiv.org/abs/2512.17129},
  html={https://arxiv.org/abs/2512.17129},
  abstract={We introduce DiffeoMorph, an end-to-end differentiable framework for learning a morphogenesis protocol that guides a population of agents to morph into a target 3D shape. Each agent updates its position and internal state using an attention-based SE(3)-equivariant graph neural network, based on its own internal state and signals received from other agents.},
